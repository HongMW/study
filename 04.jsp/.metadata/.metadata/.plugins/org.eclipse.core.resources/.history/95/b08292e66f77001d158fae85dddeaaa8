<%
class Student {
	int sno; String sname; String gender; int age;
	
	public Student(int sno, String sname, String gender, int age) {
		this.sno = sno;
		this.sname = sname;
		this.gender = gender;
		this.age = age;
		
	}
	
	Student std1 = new Student(1000, "홍길동", "남자", 43);
	Student std2 = new Student(2000, "소향", "여자", 44);
	
}
%>
