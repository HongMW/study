package com.lec.basic.poly;

public class SonySpeaker {

	private SonySpeaker speaker;
	public SonySpeaker() { System.out.println("==> sony Speaker 객체생성");}
	
	public void volumnUp() { System.out.println("==> sony Speaker 볼륨업"); }
	
	public void volumnDown() { System.out.println("==> sony Speaker 볼륨다운"); }
}
